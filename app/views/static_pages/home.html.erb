<h1>Home</h1>

<h2>Users</h2>
<table border="1" style="width:50%">
<% @users.each do |user| %>
  <tr>
    <td><%= user.username %> </td>
    <td><%= user.email %> </td>
    <td><%= user.age %> </td>
  </tr>
<% end %>
</table>

<h2>Cats</h2>
<table border="1" style="width:50%">
<% @cats.each do |cat| %>
  <tr>
    <td><%= cat.name %> </td>
  </tr>
<% end %>
</table>

<h2>Todos</h2>
<table border="1" style="width:50%">
<% @todos.each do |task| %>
  <tr>
    <td><%= task.tasks %> </td>
    <td><%= task.finished %> </td>
  </tr>
<% end %>
</table>
